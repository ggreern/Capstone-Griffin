@page
@model Capstone.Pages.AdminPage.AdminReportModel
@{
}

<h2>User Report</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Username</th>
            <th>User Type</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.AdminReportData)
        {
            <tr>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Email</td>
                <td>@item.PhoneNumber</td>
                <td>@item.Username</td>
                <td>@item.UserType</td>

            </tr>
        }
    </tbody>
</table>
